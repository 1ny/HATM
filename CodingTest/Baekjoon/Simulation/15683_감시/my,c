#include <stdio.h>

#define MAX 8

int n, m;
int board1[MAX][MAX];
int cctv[8][2];

int main() {
    int minZ = 0;

    scanf("%d %d", &n, &m);
    for(int i=0; i<n; i++) {
        for(int j=0; j<m; j++) {
            scanf("%d ", board1[i][j]);

            if(board1[i][j] == 0) minZ++;
        }
    }

    
}